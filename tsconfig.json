{
  "compilerOptions": {
    "target": "ES2018",
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "noEmit": true,
    "skipLibCheck": true,
    "strict": false
  }
}
